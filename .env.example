# Ecobee Temperature Manager Configuration
# Copy this file to .env and fill in your credentials

# Required: Ecobee account credentials
ECOBEE_EMAIL=your.email@example.com
ECOBEE_PASSWORD=your_password_here

# Optional: Service configuration
CHECK_INTERVAL_MINUTES=10
LOG_LEVEL=INFO

# Optional: Selenium timeouts (for JWT token refresh)
SELENIUM_TIMEOUT=30
SELENIUM_REDIRECT_TIMEOUT=60
